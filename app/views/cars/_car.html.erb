<div id="<%= dom_id car %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Plate number:</strong>
    <%= car.plate_number %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Model:</strong>
    <%= car.model %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Year:</strong>
    <%= car.year %>
  </p>

</div>
